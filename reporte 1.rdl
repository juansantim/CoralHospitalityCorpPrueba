  <DataSets>
    <DataSet Name="TiposProductos">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>Select null as Id, 'Todos' as Nombre
union all
Select Id, nombre from TipoProductoes</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Nombre">
          <DataField>Nombre</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Productos">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@tipoProducto">
            <Value>=Parameters!tipoProducto.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select p.Id, p.Codigo, p.Nombre, tp.Nombre TipoProducto
from Productoes p
	 inner join TipoProductoes tp on p.TipoProductoId = tp.Id
where  (@tipoProducto is null or tp.Id = @tipoProducto)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
