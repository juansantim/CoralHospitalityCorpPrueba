  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>Select p.Id, 
	   p.Codigo, 
	   p.Nombre, 
	   tp.Nombre as TipoProducto,
	   c.Nombre as Categoria
from Productoes p
	 inner join TipoProductoes tp on p.TipoProductoId = tp.Id
	 inner join TipoProductoCategorias tpc on tp.Id = tpc.TipoProductoId
	 inner join Categorias as c on tpc.CategoriaId = c.Id</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Codigo">
